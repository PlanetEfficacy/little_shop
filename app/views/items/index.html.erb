<h1>All Items</h1>
<% @items.each do |item| %>
  <div class="item">
    <%= item.title %>
    <%= item.price %>
    <%= image_tag item.image_url %>
    <%= button_to "Add to Cart", cart_path(item_id: item.id)  %>
  </div>
<% end %>
<%= link_to "View Cart", cart_path%>
<%= link_to "Login", login_path %>
